<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="reserve">
	<!-- 입력 -->
	<insert id="reserve_myinsert" parameterType="reserve.MyReserveVo">
			INSERT INTO MyReserve(myName, myJumin, myPhone, Email, reserveVaccine, reserveTime, reserveCenter, reserveNum )
			values(#{myName}, #{myJumin} , #{myPhone}, #{Email}, #{reserveVaccine},  #{reserveTime},  #{reserveCenter},  seq_reserveNum.nextVal)		
	</insert>
	
	<!-- 조회 -->
	<select id="reserve_search" parameterType="reserve.MyReserveVo" resultType="reserve.MyReserveVo">
			select * from MyReserve where myJumin = #{myJumin}
	</select>
	
	<!-- 삭제 -->
	<delete id="reserve_delete" parameterType="reserve.MyReserveVo">
			DELETE FROM MyReserve WHERE myJumin = #{myJumin}
	</delete>
	
</mapper>